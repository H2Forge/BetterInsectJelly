<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VCE_InsectJellyPreserves"]/ingestible</xpath>
						<value>
							<outcomeDoers>
								<li Class="IngestionOutcomeDoer_OffsetNeed">
									<need>Rest</need>
									<offset>0.025</offset>
								</li>
							</outcomeDoers>
						</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="VCE_InsectJellyPreserves"]/statBases/MarketValue</xpath>
						<value>
							<MarketValue>18.0</MarketValue>
						</value>
				</li>
	
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="VCE_InsectJellyPreserves"]/statBases/Nutrition</xpath>
						<value>
							<Nutrition>0.1</Nutrition>
						</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>
