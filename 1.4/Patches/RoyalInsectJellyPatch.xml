<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="VFEI_RoyalInsectJelly"]/statBases/MarketValue</xpath>
					<value>
						<MarketValue>20.0</MarketValue>
					</value>
				</li>
			
				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="VFEI_RoyalInsectJelly"]/statBases/Nutrition</xpath>
						<value>
							<Nutrition>0.1</Nutrition>
						</value>
				</li>
			
			
				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VFEI_RoyalInsectJelly"]/ingestible/outcomeDoers</xpath>
						<value>
							<li Class="IngestionOutcomeDoer_OffsetNeed">
								<need>Rest</need>
								<offset>0.05</offset>
							</li>
						</value>
				</li>
			</operations>
		</match>
	</Operation>
	
</Patch>
